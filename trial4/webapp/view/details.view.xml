<mvc:View controllerName="trial4.controller.details"
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true"
    xmlns="sap.ui.table"
	xmlns:u="sap.ui.unified"
	xmlns:c="sap.ui.core"
	xmlns:core="sap.ui.core"
	xmlns:m="sap.m" >


    <m:Page id="details" title="{i18n>title}">

        <m:content>

			<m:Button id="_IDGenButton20" text="Go Back"
				press="onPress">
				<m:layoutData>
					<m:FlexItemData id="_IDGenFlexItemData2" growFactor="1" />
				</m:layoutData>
			</m:Button>

            <m:VBox id="_IDGenVBox1" binding="{detailsModel>/}">
                <m:items>
                    <m:Text id="_IDGenText5" text="Mandt: {detailsModel>/Mandt}" />
                    <m:Text id="_IDGenText10" text="Kunnr: {detailsModel>/Kunnr}" />
                    <m:Text id="_IDGenText11" text="Name1: {detailsModel>/Name1}" />
                    <m:Text id="_IDGenText12" text="Name2: {detailsModel>/Name2}" />
                    <m:Text id="_IDGenText13" text="Stcd1: {detailsModel>/Stcd1}" />
                    <m:Text id="_IDGenText14" text="Stcd2: {detailsModel>/Stcd2}" />
                    <m:Text id="_IDGenText15" text="SmtpAddr: {detailsModel>/Smtp_addr}" />
                    <m:Text id="_IDGenText16" text="Street: {detailsModel>/Street}" />
                    <m:Text id="_IDGenText17" text="City1: {detailsModel>/City1}" />
                    <m:Text id="_IDGenText18" text="TelNumber: {detailsModel>/Tel_number}" />
                    <m:Text id="_IDGenText19" text="Stkzn: {detailsModel>/Stkzn}" />
                </m:items>
            </m:VBox>
			
		</m:content>

		


    </m:Page>

</mvc:View>